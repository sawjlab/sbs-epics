   field(OMSL,"supervisory")
   field(SCAN,"Passive")
   field(ASG,"EXPERT")
   field(EGU,"uA")
   alias("BB_DET_$(Det)_$(Sys)_$(Element):iset")
   alias("BB_SYS_$(Sys)_$(Det)_$(Element):iset")
}
