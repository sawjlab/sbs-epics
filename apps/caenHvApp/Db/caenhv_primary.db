# CAEN A1932A EPICS records
#   that are unique to primary channels
#
record(ao, "$(CrName)_Sl$(Sl)_Ch$(Ch):imon")
{
   field(DESC,"#GR")
   field(DOL, "$(CrName)_Sl$(Sl)_Ch$(Ch).E CPP NMS")
   field(OMSL,"closed_loop")
   field(PREC,"2")
   field(EGU,"uA")
   alias("BB_DET_$(Det)_$(Sys)_$(Element):imon")
   alias("BB_SYS_$(Sys)_$(Det)_$(Element):imon")
   info(autosaveFields_pass0,"HSV LSV HHSV LLSV HIGH LOW HIHI LOLO")
}   

